<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ModeItem">
        <attr name="mode_name" format="string" />
        <attr name="text_color" format="color" />
    </declare-styleable>

    <declare-styleable name="DayCell">
        <attr name="state_within_month" format="boolean" />
        <attr name="state_within_interval" format="boolean" />
        <attr name="state_continue_edge" format="boolean" />
        <attr name="state_active_edge" format="boolean" />
        <attr name="state_passive_edge" format="boolean" />
        <attr name="state_beyond_today" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TintableImageView">
        <attr name="tint" format="reference|color" />
    </declare-styleable>

</resources>